{
    "targets": [{
        "target_name": "qnode",
        "sources": [],
        "build_dir": "output",
        "include_dirs": [
            "<!(node -e \"require('nan')\")",
            "./",
            "/Users/alee/project/qnode/lib/includes",
            "/Users/alee/project/qnode/lib/includes/QtCore"
        ],
        "cflags": ["-Wno-deprecated-declarations"],


        "conditions": [
            ["OS=='linux'", {
                "cflags": [
                    "-I<!@(qmake -query 'QT_INSTALL_PREFIX')/include/QtGui/<!@(qmake -query 'QT_VERSION')/QtGui",
                    "<!@(pkg-config --cflags Qt5Core Qt5Network Qt5Gui Qt5Widgets Qt5WebEngineCore Qt5WebEngine Qt5WebEngineWidgets Qt5X11Extras Qt5SerialPort Qt5Multimedia)", "-Wcomment", "-Wparentheses",
                    "-frtti"
                ],
                "ldflags": [
                    "<!@(pkg-config --libs-only-L --libs-only-other Qt5Core Qt5Network Qt5Gui Qt5Widgets Qt5WebEngineCore Qt5WebEngine Qt5WebEngineWidgets Qt5X11Extras Qt5SerialPort Qt5Multimedia)", "-fexceptions",
                    "-Wl,-O1 -Wl,-rpath,\\$$ORIGIN:$ORIGIN"
                ],
                "libraries": [
                    "<!@(pkg-config --libs-only-l Qt5Core Qt5Network Qt5Gui Qt5Widgets Qt5WebEngineCore Qt5WebEngine Qt5WebEngineWidgets Qt5X11Extras Qt5SerialPort Qt5Multimedia)", "-fexceptions"
                ],

                "cflags!": ["-fno-exceptions", "-Wno-comment", "-Wno-parentheses"],
                "cflags_cc!": ["-fno-exceptions", "-Wno-comment", "-Wno-parentheses", "-fno-rtti"]

            }],
            ["OS=='mac'", {
                "libraries": [
                    "-Wl,-rpath,@loader_path/../Contents/Frameworks",
                    "-F<!@(qmake -query 'QT_INSTALL_PREFIX')/lib"
                ]
            }],
            ["OS=='win'", {
                "include_dirs": [],
                "libraries": []
            }]
        ]

    }]

    ,
    "qtlibs": [
        "QtCore",
        "QtNetwork",
        "QtGui",
        "QtWidgets",
        "QtQml",
        "QtQuick",
        "QtWebChannel",
        "QtPositioning",
        "QtPrintSupport",
        "QtQuickWidgets",
        "QtWebEngineCore",
        "QtWebEngine",
        "QtWebEngineWidgets",
        "QtSerialPort",
        "Qt5Multimedia"
    ]
}